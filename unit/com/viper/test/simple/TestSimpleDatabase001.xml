<?xml version="1.0" encoding="UTF-8"?>
<database name="test" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.vipersoftwareservices.com/schemas/database.xsd">
	<table name="BasicTable" table-type="table">
		<column default-value="1" description="Sample Description" id-method="sequence" persistent="true"
			primary-key="true" required="true" java-type="int" name="ID" />
		<column name="NAME" size="100" java-type="String" index-name="indexname" />
		<column name="LESSONID" java-type="int" />
		<index index-class="unique" index-type="btree" name="indexname" />
		<sql-select>select * from table</sql-select>
		<sql-update>update a="1", b="2" into table where c="3"</sql-update>
		<sql-insert>insert (a, b, c) values (1, 2, 4) from table</sql-insert>
		<sql-delete>delete table</sql-delete>
		<row>
			<cell name="id">1</cell>
			<cell name="lessonid">1</cell>
			<cell name="name">Tom Nevin</cell>
		</row>
		<row>
			<cell name="id">2</cell>
			<cell name="lessonid">2</cell>
			<cell name="name">Dana Nevin</cell>
		</row>
		<row>
			<cell name="id">3</cell>
			<cell name="lessonid">3</cell>
			<cell name="name">Christena Nevin</cell>
		</row>
	</table>
	<table name="SecondaryTable" table-type="table">
		<column default-value="1" description="Sample Description" id-method="sequence" persistent="true"
			primary-key="true" required="true" java-type="int" name="ID" />
		<column name="NAME" size="100" java-type="String" index-name="indexname" />
		<foreign-key constraint-name="constrain0002" deferrability="deferrability" foreign-catalog="test"
			foreign-table="BasicTable" unique="true" name="local-key-name" on-delete="restrict" on-update="cascade">
			<foreign-key-reference foreign-column="name" local-column="NAME" />
		</foreign-key>
		<index index-class="unique" index-type="btree" name="indexname" />
		<sql-select>select * from table</sql-select>
		<sql-update>update a="1", b="2" into table where c="3"</sql-update>
		<sql-insert>insert (a, b, c) values (1, 2, 4) from table</sql-insert>
		<sql-delete>delete table</sql-delete>
		<row>
			<cell name="id">1</cell>
			<cell name="name">Tom Nevin</cell>
		</row>
		<row>
			<cell name="id">2</cell>
			<cell name="name">Dana Nevin</cell>
		</row>
		<row>
			<cell name="id">3</cell>
			<cell name="name">Christena Nevin</cell>
		</row>
	</table>
	<table name="assignments" table-type="view">
		<column formatter="PrimaryKeyConverter" decimal-size="0" default-value="N" description="Sample Description"
			id-method="none" persistent="true" primary-key="true" required="true" java-type="String" name="id" size="2"
			validations="Validator" />
		<column formatter="PrimaryKeyConverter" decimal-size="0" default-value="N" description="Sample Description"
			persistent="true" required="true" java-type="int" name="lessonid" />
		<column formatter="PrimaryKeyConverter" default-value="N" description="Sample Description" java-type="String"
			name="active" />
		<column formatter="PrimaryKeyConverter" description="Sample Description" java-type="java.sql.Date"
			name="due_date" />
		<sql-select> select ID as id, LESSONID as lessonId, true as active, SYSDATE() as due_date from test.BasicTable order by
				NAME
			</sql-select>
	</table>
	<procedure description="sample procedure description" name="sumit" vendor="oracle">
		<source><![CDATA[PROCEDURE sumit (IN a INT, IN b INT, OUT c INT) BEGIN SET c = a + b;END;]]></source>
	</procedure>
	<user name="demo" password="pass">
	</user>
	<privilege grantee="jenny" grantor="tom" is-grantable="true" privilege="create" />
</database>